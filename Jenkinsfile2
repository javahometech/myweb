pipeline{
    agent any
        stages{
            stage("Maven Build"){
                When{
                    branch "dev"
                }
                steps{
                    sh "mvn package"
                }
            }
            stage("Deploy To dev"){
                When{
                    branch "dev"
                }
                steps{
                    echo "deploying to development server"
                }
            }
            stage("Deploy To prod"){
                When{
                    branch"master"
                }
                steps{
                    echo "deploying to master server"
                }
             }
        }
  }
